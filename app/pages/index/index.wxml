<!--index.wxml-->
<view class="container">

  <view class="status">
    <text>{{userInfo.nickName}} is taking shit...</text>
  </view>

  <view>
    <text>debug: {{debug_str}}</text>
  </view>

  <text>All Users:</text>

   <view class="finish-list" wx:for="{{status.userList}}" wx:for-item="info" wx:key="idx">
    <image class="avatar-sm" src="{{info.avatarUrl}}"></image>
  </view> 

  <button class="control-button" bindtap='reserveTap'>Reserve</button>
</view>